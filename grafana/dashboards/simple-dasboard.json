{
  "dashboard": {
    "title": "User Service - Основная метрика",
    "panels": [
      {
        "id": 1,
        "title": "Время отклика gRPC сервиса",
        "type": "timeseries",
        "targets": [
          {
            "expr": "userservice_grpc_request",
            "legendFormat": "{{status}} - {{method}}",
            "refId": "A"
          }
